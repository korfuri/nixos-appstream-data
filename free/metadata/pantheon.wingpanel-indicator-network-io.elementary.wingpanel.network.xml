  <releases>
    <release date="2022-06-30" urgency="medium" version="2.3.3">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Ensure correct tooltip on system start</li>
          <li>Prevent connecting to invalid VPNs</li>
          <li>Show correct info for WPA3 networks</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.3.2" urgency="medium" date="2021-11-23">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Prevent blank lines in VPN list</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.3.1" date="2021-10-22" urgency="medium">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Don't launch Captive Network Assistant with full connectivity</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release date="2021-07-14" version="2.3.0" urgency="medium">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Fix VPN spinning after connecting</li>
          <li>Improve detecting network encryption type</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Ellipsize long network names</li>
          <li>Show tooltip on hover</li>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.2.4" date="2020-05-28" urgency="medium">
      <description>
        <p>Updated translations</p>
      </description>
    </release>
    <release version="2.2.3" date="2020-03-27" urgency="medium">
      <description>
        <p>Prevent panel icon from showing in the dock</p>
        <p>Updated translations</p>
      </description>
    </release>
  </releases>
