  <releases>
    <release date="2022-07-11" version="6.0.6" urgency="medium">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Clear All button no longer leaves undeletable notifications</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release urgency="medium" version="6.0.5" date="2022-07-03">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Reduce delay in initially showing wingpanel when there is a large number of notifications</li>
          <li>Reduce delay in clearing all notifications when there is a large number of notifications</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release urgency="medium" version="6.0.4" date="2021-12-17">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Fix delayed startup</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="6.0.3" date="2021-12-10" urgency="medium">
      <description>
        <p>Fixes:</p>
        <ul>
          <li>Handle null message body</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release date="2021-10-26" urgency="medium" version="6.0.2">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release urgency="medium" date="2021-09-27" version="6.0.1">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Use app name as fallback title</li>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release urgency="medium" version="6.0.0" date="2021-07-15">
      <description>
        <p>Complete redesign:</p>
        <ul>
          <li>Multitouch gestures to dismiss notifications</li>
          <li>Close button on each notification</li>
          <li>Show images and badged icons</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Show tooltip on hover </li>
          <li>Performance improvements</li>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release date="2020-04-01" urgency="medium" version="2.1.4">
      <description>
        <p>Fix issues with disappearing notifications</p>
        <p>Updated translations</p>
      </description>
    </release>
    <release date="2019-11-25" urgency="medium" version="2.1.3">
      <description>
        <p>Fix a minor visual inconsistency</p>
        <p>Updated translations</p>
      </description>
    </release>
  </releases>
